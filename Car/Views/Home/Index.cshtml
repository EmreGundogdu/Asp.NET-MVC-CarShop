
@{
    ViewBag.Title = "Index";
}
<div class="container">
    <div class="row mt-3">
        <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/Content/img/Bmw2.jpg" class="d-block w-100" alt="..." height="600">
                </div>
                <div class="carousel-item">
                    <img src="/Content/img/Bmw8.jpg" class="d-block w-100" alt="..." height="600">
                </div>
                <div class="carousel-item">
                    <img src="/Content/img/MbGT.jpg" class="d-block w-100" alt="..." height="600">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <form class="row mt-3">
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">Brand</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="Brand" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">Model</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="Model" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">Min</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="Min Price" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">Max</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="Max Price" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">City</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="City" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label for="inputEmail3" class="col-sm-4 col-form-label">Status</label>
                    <div class="col-sm-8">
                        <input type="text" placeholder="Status" class="form-control">
                    </div>
                </div>
                <div class="row mb-3">
                    <label class="col-sm-4 col-form-label"></label>
                    <div class="col-sm-8">
                        <button type="submit" class="btn btn-outline-info col-12"><i class="bi bi-search"></i>Filter</button>
                    </div>
                </div>

            </form>
        </div>
        <div class="col-md-3">
            <div class="img-thumbnail mt-3">
                <a href="#"><img src="/Content/img/Bmw2.jpg" class="card-img-top" alt="..." height="240"></a>
                <div class="card-body">
                    <h5 class="card-title">Bmw | 2 Series </h5>
                    <h5 class="card-title">2017</h5>
                    <h5 class="card-title">27.000€</h5>

                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="img-thumbnail mt-3">
                <a href="#"><img src="/Content/img/Bmw8.jpg" class="card-img-top" alt="..." height="240"></a>
                <div class="card-body">
                    <h5 class="card-title">Bmw | 8 Series </h5>
                    <h5 class="card-title">2017</h5>
                    <h5 class="card-title">47.000€</h5>

                </div>
            </div>
        </div>
        <div class="col-md-3">
            <div class="img-thumbnail mt-3">
                <a href="#"><img src="/Content/img/MbGT.jpg" class="card-img-top" alt="..." height="240"></a>
                <div class="card-body">
                    <h5 class="card-title">Mercedes Benz | GT </h5>
                    <h5 class="card-title">2019</h5>
                    <h5 class="card-title">67.000€</h5>

                </div>
            </div>
        </div>
    </div>
</div>